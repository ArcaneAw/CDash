loadstring(game:HttpGet("https://raw.githubusercontent.com/ArcaneAw/boombox/main/AC", true))()

function Tool1()
local Tool1 = Instance.new("Tool")
Tool1.RequiresHandle = false
Tool1.ToolTip = "MultiDash tool"
Tool1.TextureId = "rbxassetid://2048633342"
Tool1.Name = "Multi"
local Plr2 = game.Players.LocalPlayer.Character
local Plr = game.Players.LocalPlayer
	
function Punch1()
	Plr2.Humanoid:UnequipTools()
	wait(0.01)
	Plr2.Humanoid:EquipTool(Plr.Backpack.Combat)
	wait(0.01)
	Plr2.Combat:Activate()
	TP1()
	wait(1.3)
	Plr2.Combat:Deactivate()
	wait(0.1)
	Plr2.Humanoid:EquipTool(Plr.Backpack.Multi)
end

function TP1()
	Plr.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(math.random(0, 10), 0, 0) wait(0.1)
	Plr.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(math.random(-8, 0), 0, 0) wait(0.1)
	Plr.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, math.random(0, 15), math.random(0, 12)) wait(0.1)
	Plr.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, math.random(0, 11)) wait(0.1)
	Plr.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -17) wait(1)
        Plr.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -20)
end

Tool.Activated:Connect(function()
    local Character = game:GetService("Players").LocalPlayer.Character
    local Humanoid = Character:FindFirstChildOfClass("Humanoid")

    		play(1336756135, true, true)
			local Swing = Instance.new('Animation');
			Swing.AnimationId = "rbxassetid://7861306542";
			local ld = game:GetService('Players').LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(Swing)
			ld:Play()
			ld:AdjustSpeed(0.45)
			Punch1()
    	local Char = game.Players.LocalPlayer.Character local Hum = Char:FindFirstChildOfClass("Humanoid") or Char:FindFirstChildOfClass("AnimationController")
			while true do wait()
				for i,v in next, Hum:GetPlayingAnimationTracks() do
					if v.Animation.AnimationId == "rbxassetid://2788289281" then
						v:Stop()
						for i,v in next, Hum:GetPlayingAnimationTracks() do
							if v.Animation.AnimationId == "rbxassetid://2788292075" then
							    v:Stop()
							    local Swing = Instance.new('Animation');
			                    Swing.AnimationId = "rbxassetid://7861306542";
			                    local ld = game:GetService('Players').LocalPlayer.Character:FindFirstChildWhichIsA('Humanoid'):LoadAnimation(Swing)
			                    ld:Play()
			                    ld:AdjustSpeed(0.45)
							    ld.TimePosition = 3;
								wait(0.1)
								play(558640653, true, true)
						end
					end
				end
		    end
		end
end)
Tool1.Parent = game:GetService("Players").LocalPlayer.Backpack
end
game:GetService('Players').LocalPlayer.Character:WaitForChild('FULLY_LOADED_CHAR');
wait(1);
Tool1();
game:GetService('Players').LocalPlayer.CharacterAdded:Connect(function(v)
    v:WaitForChild('FULLY_LOADED_CHAR');
    wait(1);
    Tool1();
end)
